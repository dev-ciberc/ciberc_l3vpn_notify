vrf {{ data.vpn_name}}
 address-family ipv4 unicast
  import route-target
  {{ data.vpn_rt }}
  !
  export route-target
  {{ data.vpn_rt }}
  !
 !
!
interface {{ data.interface_connect_CE }}
 vrf {{ data.vpn_name}}
 ipv4 address {{ data.vpn_int_ip }}{{ data.vpn_int_mask }}
 no shutdown
!
route-policy Policy-PASS
  pass
end-policy
!
router bgp {{ data.local_bgp_as }}
  address-family vpnv4 unicast
  vrf {{ data.vpn_name}}
   rd {{ data.vpn_rd }}
   address-family ipv4 unicast
   !
   neighbor {{ data.vpn_bpg_ce_neighbor_ip }}
     remote-as {{ data.neighbor_remote_as }}
     address-family ipv4 unicast
      route-policy Policy-PASS in
      route-policy Policy-PASS out
      as-override
      commit
    !
   !
 !
!
